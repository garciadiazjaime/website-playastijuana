{
  "name": "website-playastijuana",
  "version": "1.0.3",
  "description": "Playami GUI",
  "main": "start.js",
  "scripts": {
    "test": "npm run lint",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "build": "npm run build-be && npm run build-fe && npm run assets && npm run minify",
    "build-fe": "./node_modules/.bin/webpack",
    "build-be": "./node_modules/.bin/webpack",
    "start": "node --use_strict start.js",
    "clean": "rm ./static/css/screen.css && rm ./static/js/app.js && rm ./dist/server.js",
    "sprites": "./node_modules/.bin/gulp build:sprites",
    "lint": "./node_modules/.bin/eslint --ignore-pattern **/app-*.js, **/**/sprite.js --ext .js,.jsx src",
    "assets": "cp -rf ./build/. ./static/ && rm ./static/index.html",
    "minify-css": "./node_modules/.bin/gulp minify-css",
    "minify-js": "./node_modules/.bin/gulp minify-js",
    "clean-minify": "./node_modules/.bin/gulp clean-minify",
    "minify": "npm run clean-minify && npm run minify-css && npm run minify-js",
    "update": "git checkout master && git pull origin develop",
    "deploy": "git add . && git commit -m 'deploy' && git push upstream"
  },
  "author": "Mint IT Media",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "convict": "^1.5.0",
    "express": "^4.14.0",
    "jade": "^1.11.0",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-masonry-component": "^4.3.1",
    "react-router": "^3.0.0",
    "readable-stream": "^2.1.5",
    "rest": "^2.0.0",
    "sendgrid": "^4.7.1",
    "strict-uri-encode": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garciadiazjaime/website-playastijuana"
  },
  "private": true,
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.6.0",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^2.0.0",
    "node-sass": "^3.10.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.0.2",
    "source-map": "^0.5.6",
    "sprity": "^1.0.8",
    "sprity-js": "^1.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4",
    "webpack-sources": "^0.1.2"
  }
}
